{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "plugins": ["prettier-plugin-go-template"],
  "overrides": [
    { "files": ["*.html", "*.gohtml", "*.gotmpl"], "options": { "parser": "go-template" } }
  ]
}
