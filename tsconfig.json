{
  "extends": "astro/tsconfigs/strict",
  "include": ["src/**/*.astro", "src/**/*.tsx", "src/types/custom.d.ts"],
  "plugins": {
    "name": "@astrojs/ts-plugin"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
